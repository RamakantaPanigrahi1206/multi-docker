{
  "AWSEBDockerrunVersion": 2,
  "containerDefination": [
  {
    "name": "client",
    "image": "rpanigrahi/multi-client",
    "hostname": "client",
    "essential": false
  },
  {
    "name": "server",
    "image": "rpanigrahi/multi-server",
    "hostname": "api",
    "essential": false
  },
  {
    "name": "worker",
    "image": "rpanigrahi/multi-worker",
