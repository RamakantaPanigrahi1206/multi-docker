{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
  {
    "name": "client",
    "image": "rpanigrahi/multi-client",
    "hostname": "client",
    "essential": false,
    "memory": 128
  },
  {
    "name": "server",
    "image": "rpanigrahi/multi-server",
    "hostname": "api",
    "essential": false,
    "memory": 128
  },
  {
    "name": "worker",
    "image": "rpanigrahi/multi-worker",
    "hostname": "worker",
    "essential": false,
    "memory": 128
  },
  {
    "name": "nginx",
    "image": "rpanigrahi/multi-nginx",
    "hostname": "nginx",
    "essential": true,
    "portMppings": [
      {
        "hostPort": 80,
        "containerPort": 80
      }
    ],
    "links": ["client", "server"],
    "memory": 128
  }
  ]

}
